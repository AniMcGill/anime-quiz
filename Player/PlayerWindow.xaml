<Window x:Class="Anime_Quiz_3.Player.PlayerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PlayerWindow" Height="768" Width="1024">
    <Grid>
        <DockPanel HorizontalAlignment="Left" LastChildFill="True" Margin="0,0,0,0" VerticalAlignment="Top">
            <Frame x:Name="_leaderboardFrame" DockPanel.Dock="Right" HorizontalAlignment="Right" />
            <Frame x:Name="_playerFrame" />
        </DockPanel>
    </Grid>
</Window>
